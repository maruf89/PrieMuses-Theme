{
"compilerOptions": {
  "module": "es6",
  "target": "es5",
  "noImplicitAny": false,
  "removeComments": true,
  "preserveConstEnums": true,
  "outDir": "./assets/js/",
  "sourceMap": true,
  "allowJs": true,
  "declaration": true,
  "typeRoots": [
      "./src/_types",
      "./node_modules/@types"
    ],
  "baseUrl": ".",
  "paths": {
  	"ThirdParty/*": ["src/ThirdParty/*"],
    "@/*": ["src/Modules/*"]
  }
},
"include": ["src/**/*"],
"exclude": ["node_modules", "**/*.spec.ts"]
}